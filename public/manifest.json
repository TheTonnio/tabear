{
  "name": "Tabear",
  "version": "1.0",
  "description": "Bookmark Manager",
  "manifest_version": 2,
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "favicon-32x32.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "chrome://favicon/"
  ]
}
